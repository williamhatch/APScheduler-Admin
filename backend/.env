# 基本配置
PROJECT_NAME=APScheduler-Admin
API_V1_STR=/api/v1
SECRET_KEY=your-secret-key-for-jwt-please-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=60
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8080","http://localhost:5173"]

# 数据库配置 - 选择一种数据库
DATABASE_TYPE=mysql  # 可选: mysql, postgresql
# MySQL配置
MYSQL_SERVER=localhost
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_DB=apscheduler_admin
MYSQL_PORT=3306
# PostgreSQL配置
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=apscheduler_admin
POSTGRES_PORT=5432

# 初始管理员账号
FIRST_SUPERUSER=admin
FIRST_SUPERUSER_PASSWORD=admin
FIRST_SUPERUSER_EMAIL=admin@example.com

# APScheduler配置
APSCHEDULER_JOBSTORES=default
APSCHEDULER_EXECUTORS=default
APSCHEDULER_JOB_DEFAULTS={"coalesce": false, "max_instances": 3}
